<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/header'); -%>
    <link rel="stylesheet" href="/css/table.css">
</head>

<body>
    <table>
        <tr>
          <th>Course ID</th>
          <th>Course Name</th>
          <th>Grade</th>
        </tr>
        <% for (var i = 0; i < grades.length; i++) { %>
            <tr>
                <td><%= grades[i].course_id %></td>
                <td><%= grades[i].name %></td>
                <td><%= grades[i].grade %></td>
            </tr>
        <% } %>
      </table>
</body>
</html>
